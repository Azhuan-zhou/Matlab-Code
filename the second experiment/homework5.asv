clc;clear;
fun=@f5;
x0=[0,0,1,0,0,0];%初始点
x=fsolve(fun,x0);
L=(x(1)+x(2))^2+(2*x(1)+x(4))^2+(x(3)+2*x(2))^2+(x(2)-x(4))^2+x(5)*(x(1)+x(2)+x(3)+x(4)-1)+x(6)*(x(1)^2+x(2)^2+x(3)^2+x(4)^2-1);%构造拉格朗日函数
disp('通过构造拉格朗日函数，计算出的x值为：');
for i=1:4
    disp(['x',num2str(i),'=',num2str(x(i))]);
end
disp(['方程的最小值为:',num2str(L)])
a=solve(10*x(1)+2*x(2)+4*x(4)+x(5)+2*x(6)*x(1),2*x(1)+10*x(2)+4*x(3)+x(5)+2*x(6)*x(2),4*x(2)+2*x(3)+x(5)+2*x(6)*x(3),4*x(1)+4*x(4)-2*x(1)+x(5)+2*x(6)*x(4))